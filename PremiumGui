DropDown.Activated:Connect(function()
	local Click = Instance.new("Sound")
	Click.SoundId = "rbxassetid://9125655866"
	Click.Looped = false
	Click.Parent = workspace
	Click.Playing = true
	wait(1)
	Click:Destroy()
end)
DropDown.Activated:Connect(function()
	if OpenOption == false then
		OpenOption = true
		Options.Visible = true
	elseif OpenOption == true then
		OpenOption = false
		Options.Visible = false
	end
end)
HatDrop.Activated:Connect(function()
	local Click = Instance.new("Sound")
	Click.SoundId = "rbxassetid://9114480704"
	Click.PlaybackRegionsEnabled = true
	Click.PlaybackRegion = NumberRange.new(0.6, 60000)
	Click.Looped = false
	Click.Parent = workspace
	Click.Playing = true
	wait(1)
	Click:Destroy()
end)
HatFling.Activated:Connect(function()
	local Click = Instance.new("Sound")
