	hover.Looped = false
	hover.Parent = workspace
	hover.Playing = true
	wait(1)
	hover:Destroy()
end)
JX1DX1.Container.Interact.MouseLeave:Connect(function()
	local hoverend = Instance.new("Sound")
	hoverend.SoundId = "rbxassetid://6676616742"
	hoverend.Looped = false
	hoverend.Parent = workspace
	hoverend.PlaybackSpeed = 0.8
	hoverend.Playing = true
	wait(1)
	hoverend:Destroy()
end)
