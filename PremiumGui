loadstring(game:HttpGet("https://raw.githubusercontent.com/Pixeluted/adoniscries/main/Source.lua",true))()
local Inventory = game:GetObjects("rbxassetid://124529045053198")[1]
Inventory.Parent = game.Players.LocalPlayer.PlayerGui
--local Inventory = game.Players.LocalPlayer.PlayerGui.GuiForsakation
local Guest1337 = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents.Guest1337
local Shedletsky = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents.Shedletsky
local Taph = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents.Taph
local TwoTime = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents.TwoTime
local JohnDoe = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents.JohnDoe
local Jason = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents.Jason
local x1x1x1x1 = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents.x1x1x1x1
local coolkid = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents.c00lkidd
local JX1DX1 = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents.x1x1x1x1:Clone()
JX1DX1.Parent = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents
JX1DX1.Name = "JX1DX1"
JX1DX1.Container.Title.Text = "JX1DX1"
JX1DX1.Container.CharacterRender.Image = "rbxassetid://114622522420521"
local Killdroid = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents.x1x1x1x1:Clone()
Killdroid.Parent = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Contents
Killdroid.Name = "Killdroid"
Killdroid.Container.Title.Text = "Killdroid"
Killdroid.Container.CharacterRender.Image = "rbxassetid://79542890926398"
local SideContainer = Inventory.InventoryScreen.InventoryRoot.SideContainer
local topbar = Inventory.InventoryScreen.InventoryRoot.InventoryContainer.Topbar
local Skins = SideContainer.EquipContainer.Buttons.ViewSkins
local DropDown = Inventory.ModScreen.ModContainer.Contents.Command.StringParameter.DropdownFrame
local Options = DropDown.Options
local LimbReanim = Options.LimbReanim
local HatDrop = Options.HatDrop
local HatFling = Options.HatFling
local Killers = topbar.Killers
local Survivors = topbar.Survivors
local page = "Survivors"
local OpenOption = false
local Character = nil
local Reanimate = nil
local step
local RunService = game:GetService("RunService")
local Run = SideContainer.EquipContainer.Equip
Run.Title.Text = "Execute"

local hover = Instance.new("Sound")
hover.SoundId = "rbxassetid://6676616742"
hover.Looped = false
hover.Parent = workspace
hover.Playing = false
local hoverend = Instance.new("Sound")
hoverend.SoundId = "rbxassetid://6676616742"
hoverend.Looped = false
hoverend.Parent = workspace
hoverend.Playing = false



step = RunService.RenderStepped:Connect(function()

	if page == "Survivors" then
		Killdroid.Visible = false
		JX1DX1.Visible = false
	elseif page == "Killers" then
		Killdroid.Visible = true
		JX1DX1.Visible = true
	end
	if Character == nil then
		SideContainer.Visible = false
	else
		SideContainer.Visible = true
	end
	if OpenOption == true then
		DropDown.DropdownArrow.Rotation = 180
	elseif OpenOption == false then
		DropDown.DropdownArrow.Rotation = 0
	end
end)

topbar.Killers.Activated:Connect(function()
	page = "Killers"
	topbar.Killers.ImageColor3 = Color3.new(255, 255, 255)
	topbar.Survivors.ImageColor3 = Color3.new(0, 0, 0)
	topbar.Killers.Title.TextColor3 =  Color3.new(0, 0, 0)
	topbar.Survivors.Title.TextColor3 = Color3.new(255, 255, 255)
	local click = Instance.new("Sound")
	click.SoundId = "rbxassetid://9125655866"
	click.Looped = false
	click.Parent = workspace
	click.Playing = true
	wait(1)
	click:Destroy()

end)
topbar.Survivors.Activated:Connect(function()
	page = "Survivors"
	topbar.Killers.ImageColor3 = Color3.new(0, 0, 0)
	topbar.Survivors.ImageColor3 = Color3.new(255, 255, 255)
	topbar.Killers.Title.TextColor3 = Color3.new(255, 255, 255)
	topbar.Survivors.Title.TextColor3 = Color3.new(0, 0, 0)
	local click = Instance.new("Sound")
	click.SoundId = "rbxassetid://9125655866"
	click.Looped = false
	click.Parent = workspace
	click.Playing = true
	wait(1)
	click:Destroy()

end)

JX1DX1.Container.Interact.Activated:Connect(function()
	SideContainer.PreviewContainer.Render.Image = "rbxassetid://114622522420521"
	SideContainer.PreviewContainer.Title.Text = "JX1DX1"
	Character = "JX1DX1"
	local select1 = Instance.new("Sound")
	select1.SoundId = "rbxassetid://6676615443"
	select1.Looped = false
	select1.Parent = workspace
	select1.Playing = true
	wait(1)
	select1:Destroy()
end)
