HatDrop.Activated:Connect(function()
	OpenOption = false
	Options.Visible = false
	DropDown.ChosenValue.Title.Text = "HatDrop"
	Reanimate = "Hatdrop"
end)
HatFling.Activated:Connect(function()
	OpenOption = false
	Options.Visible = false
	DropDown.ChosenValue.Title.Text = "Hatdrop+Hatfling"
	Reanimate = "HatFling"
end)
LimbReanim.Activated:Connect(function()
	OpenOption = false
	Options.Visible = false
	DropDown.ChosenValue.Title.Text = "LimbReanim"
	Reanimate = "LimbReanim"
end)



Run.Activated:Connect(function()

	if Character == "JX1DX1" and Reanimate == nil then
		Inventory.InventoryScreen.Visible = false
		Inventory.ModScreen.Visible = true
		print(Character)

	end
	
	if Character == "Killdroid" and Reanimate == nil then
		Inventory.InventoryScreen.Visible = false
		Inventory.ModScreen.Visible = true
		print(Character)

	end
	if Character == nil then
		local errorS = Instance.new("Sound")
		errorS.SoundId = "rbxassetid://180185084"
		errorS.Looped = false
		errorS.Parent = workspace
		errorS.PlaybackSpeed = 0.8
		errorS.Playing = true
		wait(1)
		errorS:Destroy()
	end
end)
Inventory.ModScreen.ModContainer.Contents.Command.Run.Activated:Connect(function()
	if Reanimate ~= nil then
		local Click = Instance.new("Sound")
		Click.SoundId = "rbxassetid://9125655866"
		Click.Looped = false
		Click.Parent = workspace
		Click.Playing = true
		wait(1)
		Click:Destroy()
	else
		local Click = Instance.new("Sound")
		Click.SoundId = "rbxassetid://180185084"
		Click.Looped = false
		Click.Parent = workspace
		Click.Playing = true
		wait(1)
		Click:Destroy()
	end

end)
Inventory.ModScreen.ModContainer.Contents.Command.Run.Activated:Connect(function()
	if Character == "JX1DX1" and Reanimate == "Hatdrop" then
		Inventory:Destroy()
		step:Disconnect()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Nothingusefull/DonatorVersion/refs/heads/main/JX1DX1"))()
		print(Reanimate)

	elseif Character == "JX1DX1" and Reanimate == "LimbReanim" then
		Inventory:Destroy()
		step:Disconnect()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Nothingusefull/DonatorVersion/refs/heads/main/JX1DX1limb"))()
		print(Reanimate)

	elseif Character == "JX1DX1" and Reanimate == "HatFling" then
		Inventory:Destroy()
		step:Disconnect()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Nothingusefull/DonatorVersion/refs/heads/main/JX1DX1fling"))()
		print(Reanimate)

	end
end)
Inventory.ModScreen.ModContainer.Contents.Command.Run.Activated:Connect(function()
	if Character == "Killdroid" and Reanimate == "Hatdrop" then
		Inventory:Destroy()
		step:Disconnect()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/hgyfgsaiswhbdbvudhw/hey/refs/heads/main/Killdroid"))()
		print(Reanimate)

	elseif Character == "Killdroid" and Reanimate == "LimbReanim" then
		Inventory:Destroy()
		step:Disconnect()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/hgyfgsaiswhbdbvudhw/hey/refs/heads/main/KilldroidLimb"))()
		print(Reanimate)

	elseif Character == "Killdroid" and Reanimate == "HatFling" then
		Inventory:Destroy()
		step:Disconnect()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/hgyfgsaiswhbdbvudhw/hey/refs/heads/main/KilldroidNoFling"))()
		print(Reanimate)

	end
end)
