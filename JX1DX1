local DashSFXHit = getcustomasset("Forsakation/Killers/JX1DASHHIT.mp3")
local DashSFXStart = getcustomasset("Forsakation/Killers/JX1DASHSTART.mp3")
local Laugh = getcustomasset("Forsakation/Killers/JX1DX1_Laugh.mp3")
local swing = getcustomasset("Forsakation/Killers/JX1SWING.mp3")
local wallsmack = getcustomasset("Forsakation/Killers/JX1WALLSMACK.mp3")
local heart = getcustomasset("Forsakation/Killers/heart.mp3")
local dashSFXloop = getcustomasset("Forsakation/Killers/jx1dash.mp3")
local hitswing = getcustomasset("Forsakation/Killers/jx1hitswing.mp3")
local chase = getcustomasset("Forsakation/Killers/jx1Chase.mp3")
local Chasemusic = Instance.new("Sound")
Chasemusic.SoundId = chase
Chasemusic.Parent = workspace
Chasemusic.Volume = 1
Chasemusic.Looped = true
Chasemusic:Play()


local EmoteGui: ScreenGui = game:GetObjects("rbxassetid://70965231817963")[1]
EmoteGui.Parent = game.Players.LocalPlayer.PlayerGui
EmoteGui.Enabled = false
local Destroyed = false
local Mobile: ScreenGui = game:GetObjects("rbxassetid://77707674968679")[1]
Mobile.Parent = game.Players.LocalPlayer.PlayerGui
Mobile.Enabled = false
local Controls: ScreenGui = game:GetObjects("rbxassetid://85172975543013")[1]
Controls.Parent = game.Players.LocalPlayer.PlayerGui
Controls.Enabled = true
Controls.MobileContainer.Visible = false
Controls.Version.Text = "Pwned by: 14:00"


loadstring(game:HttpGet("https://github.com/CyberNinja103/brodwa/raw/refs/heads/main/EmpyreanHatdrop"))()

local Animations = game:GetObjects(getcustomasset("Forsakation/Killers/JX1DX1.rbxmx"))[1]
local PWNhud = game:GetObjects(getcustomasset("Forsakation/GUI/JX1hud.rbxmx"))[1]
PWNhud.Parent = game.Players.LocalPlayer.PlayerGui
local AnimHandler = loadstring(game:HttpGet("https://raw.githubusercontent.com/ProudNamed/SuperLightning/refs/heads/main/AnimModule/MainModule"))()
local Anim
local id
local one
local two 
local three 
local four 
local five 
local six 
local seven 
local eight 
local target
local walk = Animations.walk
local mouse1 = false
local i = Animations.idle
local sprinting1 = false
local ability = false
local run = Animations.run
local uis = game:GetService("UserInputService")	
local walking = false
local M1Left = Animations.m1
local LMB = false
local M1Right = Animations.m2
local poisonedheart = Animations.poisonedheart
local teleport = Animations.teleportanim
local DashStart = Animations.startdash
local Dashing = Animations.dash
local DashHit = Animations.attackdash
local DashWall = Animations.wallhit
local dash = false
local hurt = false
local sprinting = false
local ws = 12
local emote = false
local player =  game:GetService("Players").LocalPlayer
local mouse = player:GetMouse()
local debounce = false
local idle = true
local char = game.Players.LocalPlayer.Character
local hum: Humanoid = char:FindFirstChild("Humanoid")
local attackvel = Instance.new("VectorForce")
attackvel.Parent = char.HumanoidRootPart
attackvel.Attachment0 = char.HumanoidRootPart.RootAttachment
attackvel.Enabled = false
attackvel.Force = Vector3.new(0, 0, -15000)
hum.Animator:Destroy()
char.Animate:Destroy()
id = i
Anim = AnimHandler.new(char, i)
Anim:Play()
hum.WalkSpeed = ws
if uis.TouchEnabled == true then
	Mobile.Enabled = true
	Controls.MobileContainer.Visible = false
	Controls.SprintingButton.Visible = true
	Controls.DesktopContainer.Visible = false
elseif uis.TouchEnabled == false then
	Mobile.Enabled = false
	Controls.MobileContainer.Visible = false
	Controls.SprintingButton.Visible = false
	Controls.DesktopContainer.Visible = false
end
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift then
		sprinting1 = true
		ws = 27.1
		if ability == false then
			hum.WalkSpeed = 27.1
		end
	end
end)
uis.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift then
		sprinting1 = false
		ws = 12
		if ability == false then
			hum.WalkSpeed = 12

		end
	end
end)

local function onButton1Down()
	if id ~= M1Left and uis.TouchEnabled == false and ability == false and mouse1 == false and LMB == false then 
		mouse1 = true
		LMB = true
		local Anim2
		Anim2 = AnimHandler.new(char, M1Left)
		Anim2:Play()
		Anim:Stop()
		id = M1Left
		local audio = Instance.new("Sound")
		audio.SoundId = swing
		audio.Volume = 1
		audio.Parent = workspace
		audio:Play()
		wait(.2)
		attackvel.Enabled = true
		wait(0.1)
		attackvel.Enabled = false
		wait(.3)
		Anim2:Destroy()
		mouse1 = false
		wait(1)
		audio:Destroy()
	elseif  id ~= M1Right and uis.TouchEnabled == false and ability == false and mouse1 == false and LMB == true then 
		mouse1 = true
		LMB = false
		local Anim2
		Anim2 = AnimHandler.new(char, M1Right)
		Anim2:Play()
		Anim:Stop()
		id = M1Right
		local audio = Instance.new("Sound")
		audio.SoundId = swing
		audio.Volume = 1
		audio.Parent = workspace
		audio:Play()
		wait(.2)
		attackvel.Enabled = true
		wait(0.1)
		attackvel.Enabled = false
		wait(.3)
		Anim2:Destroy()
		mouse1 = false
		wait(1)
		audio:Destroy()
	end
end

mouse.Button1Down:Connect(onButton1Down)

uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.E and ability == false then
		local audio = Instance.new("Sound")
		audio.SoundId = Laugh
		audio.Parent = workspace
		audio.Volume = 5
		audio:Play()
		ability = true
		hum.WalkSpeed = 0.1
		local targetPosition = mouse.Hit.p
		player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition) + Vector3.new(0,4,0)
		Anim:Stop()
		Anim = AnimHandler.new(char, teleport)
		Anim:Play()
		wait(1)
		id = teleport
		ability = false
		hum.WalkSpeed = ws
		wait(1)
		audio:Destroy()
	end
end)

uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.Q and ability == false then
		ability = true
		hum.WalkSpeed = 0.1
		Anim:Stop()
		Anim = AnimHandler.new(char, poisonedheart)
		Anim:Play()
		local audio = Instance.new("Sound")
		audio.SoundId = heart
		audio.Parent = workspace
		audio.Volume = 1
		audio:Play()
		wait(1.4)
		hum.WalkSpeed = ws
		ability = false
		id = poisonedheart
		wait(1)
		audio:Destroy()
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.R and ability == false and debounce == false then
		hum.JumpPower = 0
		debounce = true
		ability = true
		hum.WalkSpeed = 0.1
		Anim:Stop()
		Anim = AnimHandler.new(char, DashStart)
		Anim:Play()
		local audio = Instance.new("Sound")
		audio.SoundId = DashSFXStart
		audio.Volume = 1
		audio.Parent = workspace
		audio:Play()
		wait(0.25)
		dash = true
		Anim:Stop()
		Anim = AnimHandler.new(char, Dashing)
		Anim.Looped = true
		Anim:Play()
		audio.SoundId = dashSFXloop
		audio.Looped = true
		audio.Name = "DashSFX"
		audio:Play()
		local c = Instance.new("BodyVelocity")
		c.MaxForce = Vector3.new(1,0,1) * 30000
		c.Parent = char.HumanoidRootPart
		c.Name = "DashVelocity"
		local counter = 10 * .3 -- the amount of seconds
		for i = counter, 0, -1 do
			if dash == true then
				task.wait(.1)
				c.Velocity = char.HumanoidRootPart.CFrame.lookVector * 50
			else return
			end
		end
		wait(.7)
		if dash == true then
			
			wait(2)
			audio:Destroy()
			c:Destroy()
			ability = false
			id = Dashing
			hum.WalkSpeed = ws
			hum.JumpPower = 50
			debounce = false
		end
	end
end)
function onTouched(hit)
	if dash == true then
		if not hit or not hit.Parent or hit.Parent.Name == game.Players.LocalPlayer.Name then return end
		local human = hit.Parent:findFirstChild("Humanoid")
		if human and human:IsA("Humanoid") then
			if workspace.DashSFX then
				workspace.DashSFX:Destroy()
			end
			dash = false
			char.HumanoidRootPart.DashVelocity:Destroy()
			Anim:Stop()
			Anim = AnimHandler.new(char, DashHit)
			Anim:Play() 
			local audio = Instance.new("Sound")
			audio.SoundId = DashSFXHit
			audio.Volume = 1
			audio.Parent = workspace
			audio:Play()
			wait(0.8)
			hum.JumpPower = 50
			ability = false
			id = DashHit
			hum.WalkSpeed = ws
			wait(2)
			debounce = false
		elseif hit:IsA("Part") and hit.CanCollide == true then
			if workspace.DashSFX then
				workspace.DashSFX:Destroy()
			end
			char.HumanoidRootPart.CanCollide = true
			dash = false
			char.HumanoidRootPart.DashVelocity:Destroy()
			Anim:Stop()
			Anim = AnimHandler.new(char, DashWall)
			Anim:Play() 
			local audio = Instance.new("Sound")
			audio.SoundId = wallsmack
			audio.Volume = 1
			audio.Parent = workspace
			audio:Play()
			wait(1)
			hum.JumpPower = 50
			char.HumanoidRootPart.CanCollide = false
			ability = false
			id = DashHit
			hum.WalkSpeed = ws
			wait(2)
			debounce = false
		end
	end
end
char.Torso.Touched:connect(onTouched)
