-- mobile Gui
local Gui = game.Players.LocalPlayer.PlayerGui.ControlsUI
local SprintButton = Gui.SprintingButton
local Punch = game.Players.LocalPlayer.PlayerGui.JX1Hud.M1_MOBILE.PunchButton
local Slots = game.Players.LocalPlayer.PlayerGui.JX1Hud.Slots
local hotbar = Slots.Hotbar.SlotsContainer
local DashHUD = hotbar.Dash.SlotButton
local PoisonedHUD = hotbar.PoisonedHeart.SlotButton
local TeleportHUD = hotbar.Teleport.SlotButton


-- Toggle Sprint
SprintButton.Activated:Connect(function()
	if sprinting1 == false then
		sprinting1 = true
		ws = 27.1
		if ability == false then
			hum.WalkSpeed = 27.1
		end		
	elseif sprinting1 == true then
		sprinting1 = false
		ws = 12
		if ability == false then
			hum.WalkSpeed = 12
		end
	end
end)
DashHUD.Activated:Connect(function()
	if ability == false and debounce == false then
		hum.JumpPower = 0
		debounce = true
		ability = true
		hum.WalkSpeed = 0.1
		Anim:Stop()
		Anim = AnimHandler.new(char, DashStart)
		Anim:Play()
		local audio = Instance.new("Sound")
		audio.SoundId = DashSFXStart
		audio.Volume = 1
		audio.Parent = workspace
		audio:Play()
		wait(0.25)
		dash = true
		Anim:Stop()
		Anim = AnimHandler.new(char, Dashing)
		Anim.Looped = true
		Anim:Play()
		audio.SoundId = dashSFXloop
		audio.Looped = true
		audio.Name = "DashSFX"
		audio:Play()
		local c = Instance.new("BodyVelocity")
		c.MaxForce = Vector3.new(1,0,1) * 30000
		c.Parent = char.HumanoidRootPart
		c.Name = "DashVelocity"
		local counter = 10 * .3 -- the amount of seconds
		for i = counter, 0, -1 do
			if dash == true then
				task.wait(.1)
				c.Velocity = char.HumanoidRootPart.CFrame.lookVector * 50
			else return
			end
		end
		wait(.7)
		if dash == true then

			wait(2)
			audio:Destroy()
			c:Destroy()
			ability = false
			id = Dashing
			hum.WalkSpeed = ws
			hum.JumpPower = 50
			debounce = false
		end
	end
end)
PoisonedHUD.Activated:Connect(function()
	if ability == false then
		ability = true
		hum.WalkSpeed = 0.1
		Anim:Stop()
		Anim = AnimHandler.new(char, poisonedheart)
		Anim:Play()
		local audio = Instance.new("Sound")
		audio.SoundId = heart
		audio.Parent = workspace
		audio.Volume = 1
		audio:Play()
		wait(1.4)
		hum.WalkSpeed = ws
		ability = false
		id = poisonedheart
		wait(1)
		audio:Destroy()
	end
end)
TeleportHUD.Activated:Connect(function()
	if ability == false then
		local audio = Instance.new("Sound")
		audio.SoundId = Laugh
		audio.Parent = workspace
		audio.Volume = 5
		audio:Play()
		ability = true
		hum.WalkSpeed = 0.1
		local targetPosition = mouse.Hit.p
		player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition) + Vector3.new(0,4,0)
		Anim:Stop()
		Anim = AnimHandler.new(char, teleport)
		Anim:Play()
		wait(1)
		id = teleport
		ability = false
		hum.WalkSpeed = ws
		wait(1)
		audio:Destroy()
	end
end)
Punch.Activated:Connect(function()
	if id ~= M1Left and ability == false and mouse1 == false and LMB == false then 
		mouse1 = true
		LMB = true
		local Anim2
		Anim2 = AnimHandler.new(char, M1Left)
		Anim2:Play()
		Anim:Stop()
		id = M1Left
		local audio = Instance.new("Sound")
		audio.SoundId = swing
		audio.Volume = 1
		audio.Parent = workspace
		audio:Play()
		wait(.2)
		attackvel.Enabled = true
		wait(0.1)
		attackvel.Enabled = false
		wait(.3)
		Anim2:Destroy()
		mouse1 = false
		wait(1)
		audio:Destroy()
	elseif  id ~= M1Right and ability == false and mouse1 == false and LMB == true then 
		mouse1 = true
		LMB = false
		local Anim2
		Anim2 = AnimHandler.new(char, M1Right)
		Anim2:Play()
		Anim:Stop()
		id = M1Right
		local audio = Instance.new("Sound")
		audio.SoundId = swing
		audio.Volume = 1
		audio.Parent = workspace
		audio:Play()
		wait(.2)
		attackvel.Enabled = true
		wait(0.1)
		attackvel.Enabled = false
		wait(.3)
		Anim2:Destroy()
		mouse1 = false
		wait(1)
		audio:Destroy()
	end
end)

